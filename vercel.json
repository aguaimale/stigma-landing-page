{
   "version": 2,
   "name": "stigma-landing-page",
   "builds": [
      {
         "src": "server.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      {
         "src": "/admin",
         "dest": "/admin.html"
      },
      {
         "src": "/producto",
         "dest": "/producto.html"
      },
      {
         "src": "/api/(.*)",
         "dest": "/server.js"
      },
      {
         "src": "/(.*)",
         "dest": "/index.html"
      }
   ],
   "env": {
      "NODE_ENV": "production"
   }
}
