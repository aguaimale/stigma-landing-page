{
   "version": 2,
   "name": "stigma-landing-page",
   "builds": [
      {
         "src": "*.html",
         "use": "@vercel/static"
      },
      {
         "src": "*.css",
         "use": "@vercel/static"
      },
      {
         "src": "*.js",
         "use": "@vercel/static"
      },
      {
         "src": "public/**",
         "use": "@vercel/static"
      }
   ],
   "routes": [
      {
         "src": "/admin",
         "dest": "/admin.html"
      },
      {
         "src": "/producto",
         "dest": "/producto.html"
      },
      {
         "src": "/public/(.*)",
         "dest": "/public/$1"
      },
      {
         "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
         "dest": "/$1"
      },
      {
         "src": "/(.*)",
         "dest": "/index.html"
      }
   ]
}
