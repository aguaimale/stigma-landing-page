{
   "version": 2,
   "name": "stigma-landing-page",
   "builds": [
      {
         "src": "server.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      {
         "src": "/admin",
         "dest": "/admin.html",
         "status": 200
      },
      {
         "src": "/producto",
         "dest": "/producto.html",
         "status": 200
      },
      {
         "src": "/api/(.*)",
         "dest": "/server.js"
      },
      {
         "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
         "dest": "/$1"
      },
      {
         "src": "/(.*)",
         "dest": "/index.html",
         "status": 200
      }
   ],
   "env": {
      "NODE_ENV": "production"
   }
}
